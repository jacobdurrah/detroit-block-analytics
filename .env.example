# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Database Configuration (for migrations)
DATABASE_URL=postgresql://user:password@host:port/database

# API Configuration
API_PORT=3001
API_HOST=localhost

# Detroit Data API Endpoints
PARCELS_API=https://services2.arcgis.com/qvkbeam7Wirps6zC/arcgis/rest/services/parcel_file_current/FeatureServer/0
ADDRESSES_API=https://services2.arcgis.com/qvkbeam7Wirps6zC/ArcGIS/rest/services/BaseUnitFeatures/FeatureServer/0
BUILDINGS_API=https://services2.arcgis.com/qvkbeam7Wirps6zC/ArcGIS/rest/services/BaseUnitFeatures/FeatureServer/2
STREETS_API=https://services2.arcgis.com/qvkbeam7Wirps6zC/ArcGIS/rest/services/BaseUnitFeatures/FeatureServer/1
GEOCODER_API=https://opengis.detroitmi.gov/opengis/rest/services/BaseUnits/BaseUnitGeocoder/GeocodeServer

# Processing Configuration
BATCH_SIZE=500
MAX_RETRIES=3
RETRY_DELAY=1000
CONCURRENT_REQUESTS=5

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Cache Configuration
CACHE_TTL=3600
ENABLE_CACHE=true

# Feature Flags
ENABLE_HISTORICAL_TRACKING=true
ENABLE_SPATIAL_INDEX=true